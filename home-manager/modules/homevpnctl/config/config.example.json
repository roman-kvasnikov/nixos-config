{
	"name": "Home-L2TP-IPSec",
	"vpn": {
		"server": "vpn.example.com",
		"login": "username",
		"password": "password",
		"psk": "preshared_key"
	},
	"healthcheck": {
		"enabled": true,
		"interval": 30
	},
	"network_detection": {
		"enabled": true,
		"methods": {
			"gateway_check": {
				"enabled": true,
				"home_gateways": ["192.168.1.1", "10.0.0.1"]
			},
			"ping_check": {
				"enabled": true,
				"home_hosts": ["192.168.1.1", "192.168.1.10"]
			},
			"wifi_check": {
				"enabled": true,
				"home_ssids": ["HomeWiFi", "MyRouter"]
			},
			"mac_check": {
				"enabled": true,
				"home_router_macs": ["aa:bb:cc:dd:ee:ff"]
			}
		}
	}
}
